<% @seasons.each do |season| %>
  <div class="season">
    <div class="header">
      <h3><%= "#{season.year}/#{season.year + 1}" %></h3>
    </div>

    <div class="container">
      <div class="row">
        <% season.magazines.each do |magazine| %>
          <div class="magazine col-lg-4">
              <h5><%= link_to magazine.title, magazine_path(magazine) %></h5>
          </div>
        <% end %>
      </div>
    </div>
  </div>
<% end %>